<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>form_datas</key>

    <properties>
        <property name="id" visibility="yes" translation="sulu_admin.id">
            <field-name>id</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>
        </property>

        <property name="receiverMail" visibility="always" searchability="yes" translation="sulu_admin.receiver.mail">
            <field-name>receiverMail</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>
        </property>

        <property name="senderMail" visibility="always" searchability="yes" translation="sulu_admin.sender.mail">
            <field-name>senderMail</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>
        </property>

        <property name="locale" visibility="always" searchability="yes" translation="sulu_admin.locale">
            <field-name>locale</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>
        </property>

        <property name="webspaceKey" visibility="always" searchability="yes" translation="sulu_admin.webspace">
            <field-name>webspaceKey</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>
        </property>

        <property name="created" type="datetime" visibility="yes" searchability="yes" translation="sulu_admin.created">
            <field-name>created</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>

            <filter type="datetime" />
        </property>

        <property name="changed" type="datetime" visibility="yes" searchability="yes" translation="sulu_admin.changed">
            <field-name>changed</field-name>
            <entity-name>Alengo\Bundle\AlengoFormBundle\Entity\FormData</entity-name>

            <filter type="datetime" />
        </property>
    </properties>
</list>